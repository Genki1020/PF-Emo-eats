<h1>エモ投稿</h1>
<% if @eatery.errors.any? %>
  <%= @eatery.errors.count %>件のエラーが発生しました
  <% @eatery.errors.full_messages.each do |message| %>
    <%= message %>
  <% end %>
<% end %>
<%= form_with model: @eatery, local:true do |f| %>
  <h4>画像</h4>
  <%= f.attachment_field :image %>
  <h4>ショップ名</h4>
  <%= f.text_field :eatery_name %>
  <h4>住所</h4>
  <%= f.text_area :adress%>
  <h4>説明</h4>
  <%= f.text_area :report%>
  <%= f.submit '投稿' %>
<% end %>
